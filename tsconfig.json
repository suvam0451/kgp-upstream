{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2017",
    "jsx": "preserve",
    "lib": ["dom", "es2017"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true
  },
  "include": ["./src/**/*"],
  "files": ["./functions-ts/ytcard-read.ts"],
  "exclude": ["node_modules"]
}
