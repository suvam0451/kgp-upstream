{
  "name": "gatsby-starter-headless-golang",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Debashish Patra <suvam0451@outlook.com>",
  "keywords": [
    "gatsby",
    "starters",
    "headless"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:server": "tsc functions-ts/ytcard-read.ts --outDir ./functions && tsc functions-ts/ytcard-update.ts --outDir ./functions && netlfy-lambda build functions",
    "build:client": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "gatsby develop",
    "run:lambda": "netlify-lambda serve functions",
    "run:lambdax": "tsc ./functions-ts/ytcard-read.ts --outDir ./functions && tsc functions-ts/ytcard-update.ts --outDir ./functions && netlify-lambda serve functions",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.28.2",
    "@blueprintjs/icons": "^3.18.1",
    "@blueprintjs/select": "^3.13.3",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@types/js-search": "^1.4.0",
    "@types/react-slick": "^0.23.4",
    "axios": "^0.19.2",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "faunadb": "^2.14.2",
    "gatsby": "^2.10.0",
    "gatsby-image": "^2.2.1",
    "gatsby-plugin-canonical-urls": "^2.1.0",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-postcss": "^2.3.4",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.0",
    "gatsby-remark-prismjs": "^3.3.0",
    "gatsby-remark-responsive-iframe": "^2.2.0",
    "gatsby-remark-smartypants": "^2.1.0",
    "gatsby-source-filesystem": "^2.1.0",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-remark": "^2.5.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "js-search": "^2.0.0",
    "modern-normalize": "^0.5.0",
    "netlify-cli": "^2.19.0",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "postcss-import": "^12.0.1",
    "prism-themes": "^1.1.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-slick": "^0.26.1",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3"
  }
}
